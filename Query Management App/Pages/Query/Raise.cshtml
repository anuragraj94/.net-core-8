@page
@model Query_Management_App.Pages.Query.RaiseModel
@{
    ViewData["Title"] = "Raise Query";
}


<h2>Raise Query</h2>

<form asp-action="Raise" method="post">
    <div class="form-group">
        <label for="Requestor">Requestor Name</label>
        <input type="text" class="form-control" id="Requestor" name="requestor" required />
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>